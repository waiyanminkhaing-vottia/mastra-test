# Environment Variables Template

# Logging configuration
LOG_LEVEL=debug

# Cache configuration
AGENT_CONFIG_CACHE_TTL=10  # Change detection interval in seconds (how often to check for changes)
MCP_CACHE_TTL=10          # MCP client cache TTL in seconds (5 minutes)

# Database configuration
DATABASE_URL=postgresql://username:password@localhost:5432/mastra_test
AGENT_DATABASE_URL=postgresql://username:password@localhost:5432/mastra_agents

# API Keys
OPENAI_API_KEY=your-openai-api-key
