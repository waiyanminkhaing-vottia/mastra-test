# Environment Variables Template

# Logging configuration
LOG_LEVEL=debug

# Cache configuration
# Change detection intervals (in seconds)
AGENT_CONFIG_CACHE_TTL=10
SUB_AGENT_CACHE_TTL=300
MCP_CACHE_TTL=300

# Data cache TTL (in seconds) - Safety net for change detection failures
AGENT_CONFIG_DATA_TTL=86400
SUB_AGENT_DATA_TTL=86400
MCP_DATA_TTL=86400

# Database configuration
DATABASE_URL=
AGENT_DATABASE_URL=

# API Keys
OPENAI_API_KEY=

# Tenant configuration
TENANT_ID=

