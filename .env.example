# Environment Variables Template

# Logging configuration
LOG_LEVEL=debug

# Database configuration
DATABASE_URL=
AGENT_DATABASE_URL=

# API Keys
OPENAI_API_KEY=
AZURE_API_KEY=
AZURE_RESOURCE_NAME=
AZURE_ENDPOINT=

# Tenant configuration
TENANT_ID=

# Model Manager Cache Configuration
MODEL_CACHE_TTL=3600 # Cache TTL in seconds (default: 1 hour)
MODEL_CACHE_CHECK_PERIOD=600 # Check for expired keys every N seconds (default: 10 minutes)

# Tool Manager Cache Configuration
TOOL_CACHE_TTL=3600 # Cache TTL in seconds (default: 1 hour)
TOOL_CACHE_CHECK_PERIOD=600 # Check for expired keys every N seconds (default: 10 minutes)

# Agent Manager Cache Configuration
AGENT_CACHE_TTL=3600 # Cache TTL in seconds (default: 1 hour)
AGENT_CACHE_CHECK_PERIOD=600 # Check for expired keys every N seconds (default: 10 minutes)

# MCP Client Manager Cache Configuration
MCP_CACHE_TTL=3600 # Cache TTL in seconds (default: 1 hour)
MCP_CACHE_CHECK_PERIOD=600 # Check for expired keys every N seconds (default: 10 minutes)

# Main agent name
MAIN_AGENT_NAME=main-agent
