{
  "name": "mastra-test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "PORT=4000 mastra dev --env .env.local",
    "build": "mastra build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "check-all": "pnpm type-check && pnpm lint && pnpm format:check",
    "prepare": "husky",
    "prisma:generate": "prisma generate --schema=node_modules/mastra-test-common/packages/prisma/prisma/schema.prisma"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.17",
    "@ai-sdk/google": "^2.0.14",
    "@ai-sdk/openai": "^2.0.30",
    "@mastra/core": "^0.17.1",
    "@mastra/loggers": "^0.10.12",
    "@mastra/mcp": "^0.13.0",
    "@mastra/memory": "^0.15.2",
    "@mastra/pg": "^0.16.0",
    "@prisma/client": "^6.16.2",
    "dotenv": "^17.2.2",
    "mastra-test-common": "git+https://github.com/waiyanminkhaing-vottia/mastra-test-common.git",
    "node-cache": "^5.1.2",
    "ollama-ai-provider-v2": "^1.3.1",
    "openapi-types": "^12.1.3",
    "pg-query-stream": "4.10.3",
    "react": "^19.1.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^58.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "mastra": "^0.13.1",
    "prettier": "^3.6.2",
    "prisma": "^6.16.2",
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "overrides": {
      "ai": "5.0.28"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
