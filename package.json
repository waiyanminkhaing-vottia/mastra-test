{
  "name": "mastra-test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "PORT=4000 mastra dev --env .env.local",
    "build": "mastra build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "check-all": "pnpm type-check && pnpm lint && pnpm format:check",
    "prepare": "husky",
    "prisma:generate": "prisma generate --schema=node_modules/mastra-test-common/packages/prisma/prisma/schema.prisma"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.23",
    "@ai-sdk/google": "^2.0.17",
    "@ai-sdk/openai": "^2.0.42",
    "@mastra/core": "^0.20.0",
    "@mastra/loggers": "^0.10.15",
    "@mastra/mcp": "^0.13.3",
    "@mastra/memory": "^0.15.5",
    "@mastra/pg": "^0.17.1",
    "@prisma/client": "^6.16.3",
    "dotenv": "^17.2.3",
    "mastra-test-common": "git+https://github.com/waiyanminkhaing-vottia/mastra-test-common.git",
    "node-cache": "^5.1.2",
    "ollama-ai-provider-v2": "^1.4.1",
    "openapi-types": "^12.1.3",
    "pg-query-stream": "4.10.3",
    "react": "^19.2.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^60.8.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "mastra": "^0.14.1",
    "prettier": "^3.6.2",
    "prisma": "^6.16.3",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "ai": "5.0.28"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "zod": "3"
      }
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
